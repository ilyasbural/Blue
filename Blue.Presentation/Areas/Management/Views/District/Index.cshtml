@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Management/Views/Shared/Layout.cshtml";
}
@model Tuple<List<Blue.Presentation.DistrictViewModel>>

<section class="post-wrapper-top dm-shadow clearfix">
    <div class="container">
        <div class="post-wrapper-top-shadow">
            <span class="s1"></span>
        </div>
        <div class="col-lg-12">
            <ul class="breadcrumb">
                <li><a href="/management">Home</a></li>
                <li><a href="/management/district">District</a></li>
            </ul>
        </div>
    </div>
</section>

@foreach (Blue.Presentation.DistrictViewModel DistrictViewModel in Model.Item1)
{
    <h1><a href="/management/district/update/@DistrictViewModel.Id">@DistrictViewModel.Id</a></h1>
    <h1><a href="/management/district/delete/@DistrictViewModel.Id">@DistrictViewModel.Id</a></h1>
    <br />
}