@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Management/Views/Shared/Layout.cshtml";
}
@model Tuple<List<Blue.Presentation.RealEstateViewModel>>

<section class="post-wrapper-top dm-shadow clearfix">
    <div class="container">
        <div class="post-wrapper-top-shadow">
            <span class="s1"></span>
        </div>
        <div class="col-lg-12">
            <ul class="breadcrumb">
                <li><a href="/management">Home</a></li>
                <li><a href="/management/realestate">RealEstates</a></li>
            </ul>
        </div>
    </div>
</section>

@foreach (Blue.Presentation.RealEstateViewModel RealEstateViewModel in Model.Item1)
{
    <h1><a href="/management/realestate/update/@RealEstateViewModel.Id">@RealEstateViewModel.Id</a></h1>
}