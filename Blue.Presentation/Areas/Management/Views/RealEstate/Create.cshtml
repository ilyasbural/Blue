@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Management/Views/Shared/Layout.cshtml";
}
@model Tuple<Blue.Presentation.RealEstateViewModel, Blue.Presentation.RealEstateDetailViewModel, Blue.Presentation.RealEstateComplexModel>

<section class="generalwrapper dm-shadow clearfix">
    <div class="container">
        <div class="row">
            <div id="content" class="col-lg-9 col-md-6 col-sm-6 col-xs-12 clearfix">
                <div class="property_wrapper boxes clearfix">
                    <form asp-controller="realestate" asp-action="create" method="post">
                        <label for="type">BuildingType</label>
                        @Html.DropDownListFor(Model => Model.Item1.BuildingTypeViewModel.Id, new SelectList(Model.Item3.BuildingTypeDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="status">Status</label>
                        @Html.DropDownListFor(Model => Model.Item1.BuyingTypeViewModel.Id, new SelectList(Model.Item3.BuyingTypeDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="country">Country</label>
                        @Html.DropDownListFor(Model => Model.Item1.CityViewModel.Id, new SelectList(Model.Item3.CityDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="district">District</label>
                        @Html.DropDownListFor(Model => Model.Item1.DistrictViewModel.Id, new SelectList(Model.Item3.DistrictDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="featuresaround">FeaturesAround</label>
                        @Html.DropDownListFor(Model => Model.Item1.FeaturesAroundViewModel.Id, new SelectList(Model.Item3.FeaturesAroundDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="featuresinside">FeaturesInside</label>
                        @Html.DropDownListFor(Model => Model.Item1.FeaturesInsideViewModel.Id, new SelectList(Model.Item3.FeaturesInsideDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="featuresoutside">FeaturesOutside</label>
                        @Html.DropDownListFor(Model => Model.Item1.FeaturesOutsideViewModel.Id, new SelectList(Model.Item3.FeaturesOutsideDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="fromwho">FromWho</label>
                        @Html.DropDownListFor(Model => Model.Item1.FromWhoViewModel.Id, new SelectList(Model.Item3.FromWhoDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="fueltype">FuelType</label>
                        @Html.DropDownListFor(Model => Model.Item1.FuelTypeViewModel.Id, new SelectList(Model.Item3.FuelTypeDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="furniture">Furniture</label>
                        @Html.DropDownListFor(Model => Model.Item1.FurnitureViewModel.Id, new SelectList(Model.Item3.FurnitureDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="homwtown">Hometown</label>
                        @Html.DropDownListFor(Model => Model.Item1.HometownViewModel.Id, new SelectList(Model.Item3.HometownDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="price">Price</label>
                        @Html.DropDownListFor(Model => Model.Item1.PriceViewModel.Id, new SelectList(Model.Item3.PriceDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="room">Room</label>
                        @Html.DropDownListFor(Model => Model.Item1.RoomViewModel.Id, new SelectList(Model.Item3.RoomDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="size">Size</label>
                        @Html.DropDownListFor(Model => Model.Item1.SizeViewModel.Id, new SelectList(Model.Item3.SizeDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="status">Status</label>
                        @Html.DropDownListFor(Model => Model.Item1.StatusViewModel.Id, new SelectList(Model.Item3.StatusDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="type">Type</label>
                        @Html.DropDownListFor(Model => Model.Item1.TypeViewModel.Id, new SelectList(Model.Item3.TypeDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="warming">Warming</label>
                        @Html.DropDownListFor(Model => Model.Item1.WarmingViewModel.Id, new SelectList(Model.Item3.WarmingDataSource, "Id", "Name"), "Seçilmedi", new { @class = "show-menu-arrow selectpicker" })
                        <label for="status">Name</label>
                        @Html.EditorFor(m => m.Item1.Name, new { htmlAttributes = new { @class = "form-control", @placeholder = "realestate name" } })

                        <button type="button" class="btn btn-success"><i class="fa fa-save"></i></button>
                        <button type="submit" class="btn btn-primary">Kaydet <i class="fa fa-send"></i></button>
                    </form>
                </div>
            </div>

