			@Html.DropDownListFor(Model => Model.Item1.TypeViewModel.Id, new SelectList(Model.Item3.TypeDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
			@Html.DropDownListFor(Model => Model.Item1.WarmingViewModel.Id, new SelectList(Model.Item3.WarmingDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
			@* @Html.EditorFor(m => m.Item1.Name, new { htmlAttributes = new { @class = "form-control", @placeholder = "adı" } }) *@
		</div>
