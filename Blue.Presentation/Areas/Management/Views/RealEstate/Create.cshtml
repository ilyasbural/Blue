		<h4 class="modal-title">Emlak Bilgileri</h4>
	</div>
	<div class="modal-body">
		<div class="form-group">
			
			@Html.DropDownListFor(Model => Model.Item1.BuyingTypeViewModel.Id, new SelectList(Model.Item3.BuyingTypeDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
			@Html.DropDownListFor(Model => Model.Item1.CityViewModel.Id, new SelectList(Model.Item3.CityDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
			@Html.DropDownListFor(Model => Model.Item1.DistrictViewModel.Id, new SelectList(Model.Item3.DistrictDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
			@Html.DropDownListFor(Model => Model.Item1.FeaturesAroundViewModel.Id, new SelectList(Model.Item3.FeaturesAroundDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
			@Html.DropDownListFor(Model => Model.Item1.FeaturesInsideViewModel.Id, new SelectList(Model.Item3.FeaturesInsideDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
			@Html.DropDownListFor(Model => Model.Item1.FeaturesOutsideViewModel.Id, new SelectList(Model.Item3.FeaturesOutsideDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
			@Html.DropDownListFor(Model => Model.Item1.FromWhoViewModel.Id, new SelectList(Model.Item3.FromWhoDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
			@Html.DropDownListFor(Model => Model.Item1.FuelTypeViewModel.Id, new SelectList(Model.Item3.FuelTypeDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
			@Html.DropDownListFor(Model => Model.Item1.FurnitureViewModel.Id, new SelectList(Model.Item3.FurnitureDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
			@Html.DropDownListFor(Model => Model.Item1.HometownViewModel.Id, new SelectList(Model.Item3.HometownDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
			@Html.DropDownListFor(Model => Model.Item1.PriceViewModel.Id, new SelectList(Model.Item3.PriceDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
			@Html.DropDownListFor(Model => Model.Item1.RoomViewModel.Id, new SelectList(Model.Item3.RoomDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
			@Html.DropDownListFor(Model => Model.Item1.SizeViewModel.Id, new SelectList(Model.Item3.SizeDataSource, "Id", "Name"), "Seçilmedi", new { @class = "form-control" })
